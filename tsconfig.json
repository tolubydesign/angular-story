{
  "compileOnSave": false,
  "compilerOptions": {
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "baseUrl": "./",
    "paths": {
      "@data/*": [ "./src/assets/data/*" ],
      "@services/*": [ "./src/app/services/*" ],
      "@models/*": [ "./src/app/shared/models/*" ],
    },
  },
  "angularCompilerOptions": {
    "strictTemplates": true,
  }
}